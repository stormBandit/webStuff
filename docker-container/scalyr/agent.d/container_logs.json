{
  // Log files to upload to Scalyr. You can use '*' wildcards here.
  logs: [
    {
      path: "/var/log/myapp/*",
      attributes: {
        parser: "json"
      }
    },
    {
      path: "/var/log/nginx/access_*.log",
      attributes: {
        parser: "accessLog"
      }
    },
    {
      path: "/var/log/nginx/error_*.log",
      attributes: {
        parser: "nginxErrorLog"
      }
    }
  ]
}